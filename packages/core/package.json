{
	"name": "@decoco/core",
	"version": "0.3.0",
	"description": "decoco is an efficient Web Component framework based on decorator-driven development",
	"main": "./dist/index.esm.dev.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"development": {
				"types": "./dist/index.d.ts",
				"import": "./dist/index.esm.dev.js",
				"require": "./dist/index.umd.dev.js"
			},
			"production": {
				"types": "./dist/index.d.ts",
				"import": "./dist/index.esm.prod.js",
				"require": "./dist/index.umd.prod.js"
			}
		}
	},
	"scripts": {
		"dev": "rollup -c -w",
		"build": "tsc --noEmit && rollup -c --environment Production",
		"prepublish": "pnpm run build"
	},
	"packageManager": "pnpm@8.7.0",
	"author": "star-ll",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/star-ll/deco.git"
	},
	"license": "MIT",
	"files": [
		"dist",
		"package.json",
		"README.MD"
	],
	"keywords": [
		"decoco",
		"@decoco",
		"@decoco/core",
		"web component",
		"decorators",
		"component"
	],
	"engines": {
		"node": ">=16.0.0"
	},
	"dependencies": {
		"@decoco/renderer": "workspace:*",
		"reflect-metadata": "^0.2.2"
	},
	"devDependencies": {
		"@babel/core": "^7.24.3",
		"@babel/plugin-proposal-decorators": "^7.24.7",
		"@babel/plugin-transform-runtime": "^7.24.7",
		"@babel/preset-env": "^7.24.3",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/node": "^20.11.30",
		"@typescript-eslint/eslint-plugin": "^7.3.1",
		"@typescript-eslint/parser": "^7.3.1",
		"rollup": "^4.19.0",
		"rollup-plugin-dts": "^6.1.1",
		"rollup-plugin-node-externals": "^7.1.2",
		"rollup-plugin-visualizer": "^5.12.0",
		"typescript": "^5.4.3"
	}
}
